	year = {2000},
	pages = {657--668},
	file = {Mechanic and Meyer - 2000 - Concepts of trust among patients with serious illn.pdf:/Users/alistairthorpe/Zotero/storage/9Z7M2FR6/Mechanic and Meyer - 2000 - Concepts of trust among patients with serious illn.pdf:application/pdf}
