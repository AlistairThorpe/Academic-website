	file = {Stolow et al. - 2020 - How Fear Appeal Approaches in COVID-19 Health Comm.pdf:/Users/alistairthorpe/Zotero/storage/QGDT7YKL/Stolow et al. - 2020 - How Fear Appeal Approaches in COVID-19 Health Comm.pdf:application/pdf}
