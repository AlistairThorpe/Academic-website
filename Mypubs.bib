	file = {Scherer et al. - 2016 - Development of the Medical Maximizer-Minimizer Sca.pdf:/Users/alistairthorpe/Zotero/storage/8ZPNUTMM/Scherer et al. - 2016 - Development of the Medical Maximizer-Minimizer Sca.pdf:application/pdf}
}

