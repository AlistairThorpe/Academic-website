	file = {Bagger et al. - 2015 - Inappropriate antibiotic prescribing and demand fo.pdf:/Users/alistairthorpe/Zotero/storage/EKDY7KQY/Bagger et al. - 2015 - Inappropriate antibiotic prescribing and demand fo.pdf:application/pdf}
