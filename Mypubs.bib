	file = {Waters et al. - 2007 - Aversion to side effects in preventive medical tre.pdf:/Users/alistairthorpe/Zotero/storage/NFJHEI5F/Waters et al. - 2007 - Aversion to side effects in preventive medical tre.pdf:application/pdf}
