@article{rubin_multifaceted_2005,
	title = {A {Multifaceted} {Intervention} to {Improve} {Antimicrobial} {Prescribing} for {Upper} {Respiratory} {Tract} {Infections} in a {Small} {Rural} {Community}},
	volume = {40},
	issn = {1058-4838, 1537-6591},
	url = {https://academic.oup.com/cid/article-lookup/doi/10.1086/427500},
	doi = {10.1086/427500},
	abstract = {Background. Antibiotic prescribing for upper respiratory tract infections (URTIs) is widespread, is often inappropriate, and may contribute to antibiotic resistance among community-acquired pathogens, such as Streptococcus pneumoniae.
Methods. A multifaceted intervention involving health care professionals and patients was introduced to a small rural Utah community and included the repetitive use of printed diagnostic and treatment algorithms by professionals. Data on the quantity and class of antibiotic prescribing, which were collected from multiple sources, were measured for the intervention period (from January through June) in 2001 and compared with data for the baseline period during the same months in 2000.
Results. Medicaid claims data revealed that the percentage of patients in the community who received antibiotics for URTIs during the intervention period was 15.6\% less than that for the baseline period, whereas the percentage in the rest of rural Utah was relatively stable, with a 1.5\% decrease (P p .006). The greatest impact of the intervention was on prescribing for acute bronchitis (decreases of 56.1\% and 1.7\% in the community and rural Utah, respectively; P p .024) and on prescribing of macrolides (decreases of 13.4\% and 0.2\% in the community and rural Utah, respectively; P ! .001). Community pharmacy data likewise revealed a 17.5\% decrease in the rate of antibiotic prescribing during the intervention period (P ! .001), with the largest decrease observed for macrolide prescribing (50.9\%; P ! .001). Chart review data, in contrast, revealed no signiﬁcant decrease in the percentage of patients with URTI who were prescribed an antibiotic (3.8\%; P p .49), although there was a signiﬁcant decrease of 11.2\% in macrolide use (P p .045).
Conclusions. A multifaceted intervention involving the repetitive use of printed algorithms resulted in modest improvements in antibiotic prescribing for outpatient URTIs, although one data source did not corroborate this. However, macrolide prescribing decreased sharply, irrespective of the source of data.},
	language = {en},
	number = {4},
	urldate = {2020-08-06},
	journal = {Clinical Infectious Diseases},
	author = {Rubin, M. A. and Bateman, K. and Alder, S. and Donnelly, S. and Stoddard, G. J. and Samore, M. H.},
	month = feb,
	year = {2005},
	pages = {546--553},
	file = {Rubin et al. - 2005 - A Multifaceted Intervention to Improve Antimicrobi.pdf:/Users/alistairthorpe/Zotero/storage/WZHXYC8U/Rubin et al. - 2005 - A Multifaceted Intervention to Improve Antimicrobi.pdf:application/pdf}
}

