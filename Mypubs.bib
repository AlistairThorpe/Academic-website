	file = {Francis - 2012 - The Psychology of Replication and Replication in P.pdf:/Users/alistairthorpe/Zotero/storage/XZGM7B7A/Francis - 2012 - The Psychology of Replication and Replication in P.pdf:application/pdf}
}

