	abstract = {Nudge interventions – behaviorally-motivated design changes with no ﬁnancial incentives – have quickly expanded from academic studies to larger implementations in so-called Nudge Units in governments. This provides a unique opportunity to compare interventions in research studies, versus at scale. In this paper, we assemble a unique data set including all trials run by two of the largest Nudge Units in the United States, including 126 RCTs covering over 23 million individuals. We compare these trials to a separate sample of nudge trials published in academic journals from two recent meta-analyses. In papers published in academic journals, the average impact of a nudge is very large – an 8.7 percentage point take-up increase over the control. In the Nudge Unit trials, the average impact is still sizable and highly statistically signiﬁcant, but smaller at 1.4 percentage points. We show that a large share of the gap is accounted for by publication bias, exacerbated by low statistical power, in the sample of published papers; in contrast, the Nudge Unit studies are well-powered, a hallmark of “at scale” interventions. Accounting for publication bias, and some diﬀerences in characteristics, reconciles the two estimates. We also compare these results to the predictions of academics and practitioners. Most forecasters overestimate the impact for the Nudge Unit interventions, though nudge practitioners are almost perfectly calibrated.},
	language = {en},
	number = {w27594},
	urldate = {2020-08-30},
	institution = {National Bureau of Economic Research},
	author = {DellaVigna, Stefano and Linos, Elizabeth},
	month = jul,
	year = {2020},
	doi = {10.3386/w27594},
	pages = {w27594},
	file = {DellaVigna and Linos - 2020 - RCTs to Scale Comprehensive Evidence from Two Nud.pdf:/Users/alistairthorpe/Zotero/storage/6VUXUUW9/DellaVigna and Linos - 2020 - RCTs to Scale Comprehensive Evidence from Two Nud.pdf:application/pdf}
}

@article{sindermann_should_2020,
	title = {Should {We} {Pay} for {Our} {Social} {Media}/{Messenger} {Applications}? {Preliminary} {Data} on the {Acceptance} of an {Alternative} to the {Current} {Prevailing} {Data} {Business} {Model}},
	volume = {11},
	issn = {1664-1078},
	shorttitle = {Should {We} {Pay} for {Our} {Social} {Media}/{Messenger} {Applications}?},
	url = {https://www.frontiersin.org/articles/10.3389/fpsyg.2020.01415/full},
	doi = {10.3389/fpsyg.2020.01415},
	abstract = {In the age of surveillance capitalism, the prevailing business model underlying the use of social media applications (“apps”) foresees the exchange of personal data for the allowance to use an online service. Such a data business model comes with many potential negative side effects ranging from violation of privacy issues to election manipulation. Therefore, it is of utmost importance to think of alternatives to the current data business model. The present study investigated how strong the support would be for a monetary payment model among a sample of 210 participants. Participants were asked about their willingness to pay for social media, if in turn their data would be private and other problems concerning social media use would be tackled. Only one-fifth of participants (21.43\%) supported such a model. From the Big Five Personality traits, Agreeableness was positively associated with support of such a model. Finally, data are also provided on how much participants would be willing to pay for social media on a monthly basis. The present study’s findings are of a preliminary nature and will contribute to the start of an important discussion.},
	language = {English},
	urldate = {2020-08-30},
	journal = {Frontiers in Psychology},
	author = {Sindermann, Cornelia and Kuss, Daria J. and Throuvala, Melina A. and Griffiths, Mark D. and Montag, Christian},
	year = {2020},
	note = {Publisher: Frontiers},
	keywords = {big five, Big Five, data business model, payment model, Payment model, personality, Personality, social media, Social Media, surveillance capitalism, Surveillance capitalism},
	file = {Full Text PDF:/Users/alistairthorpe/Zotero/storage/UD9LD3S6/Sindermann et al. - 2020 - Should We Pay for Our Social MediaMessenger Appli.pdf:application/pdf}
}

@article{lorenz-spreen_how_2020,
