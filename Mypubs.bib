}

@article{pennycook_fighting_nodate,
	title = {Fighting {COVID}-19 misinformation on social media: {Experimental} evidence for a scalable accuracy nudge intervention},
	abstract = {Across two studies with over 1,600 U.S. adults recruited online, we present evidence that people share false claims about COVID-19 partly because they simply fail to think sufficiently about whether or not content is accurate when deciding what to share. In Study 1, participants were far worse at discerning between true and false content when deciding what they would share on social media relative to when they are asked directly about accuracy. Furthermore, cognitive reflection and science knowledge were associated with stronger discernment. In Study 2, we found that a simple accuracy reminder at the beginning of the study – i.e., judging the accuracy of a nonCOVID-19-related headline – more than doubled the level of truth discernment in participants’ sharing intentions. Our results, which mirror those found previously for political fake news, suggest that nudging people to think about accuracy is a simple way to improve choices about what to share on social media.},
	language = {en},
	author = {Pennycook, Gordon and McPhetres, Jonathon and Zhang, Yunhao and Lu, Jackson G and Rand, David G},
	pages = {28},
	file = {Pennycook et al. - Fighting COVID-19 misinformation on social media .pdf:/Users/alistairthorpe/Zotero/storage/32QPR6I3/Pennycook et al. - Fighting COVID-19 misinformation on social media .pdf:application/pdf}
