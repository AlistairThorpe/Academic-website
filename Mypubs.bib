	pages = {537--543},
	file = {Lambert et al. - 2007 - Can mass media campaigns change antimicrobial pres.pdf:/Users/alistairthorpe/Zotero/storage/IPZJFQ4L/Lambert et al. - 2007 - Can mass media campaigns change antimicrobial pres.pdf:application/pdf}
}

