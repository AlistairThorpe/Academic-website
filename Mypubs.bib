@article{hagger_predicting_2016,
	title = {Predicting {Self}-{Management} {Behaviors} in {Familial} {Hypercholesterolemia} {Using} an {Integrated} {Theoretical} {Model}: the {Impact} of {Beliefs} {About} {Illnesses} and {Beliefs} {About} {Behaviors}},
	volume = {23},
	issn = {1070-5503, 1532-7558},
