	number = {1},
	urldate = {2020-08-06},
	journal = {Antimicrobial Resistance and Infection Control},
	author = {Chalfine, Annie and Kitzis, Marie-Dominique and Bezie, Yvonnick and Benali, Adel and Perniceni, Laurence and Nguyen, Jean-Claude and Dumay, Marie and Gonot, Jacqueline and Rejasse, Gilles and Goldstein, Fred and Carlet, Jean and Misset, Benoît},
